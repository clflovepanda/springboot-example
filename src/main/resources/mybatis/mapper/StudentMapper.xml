<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.panda.springbootexample.dao.StudentDao">
    <select id="getStudentById" resultType="com.panda.springbootexample.domain.Student">
        select * from student where id = #{id};
    </select>
</mapper>